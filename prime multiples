factors, primes=[1],[]
def prime(x,y):
    for x in range(2,x):
        if y%x!=0:
            primes.append(y)
#error in function prime    
    
def prime_factor(x):
    for n in range(2, int(x**.5)+1):
#got idea for sqrt function from online forum; makes code much quicker.
        if x%n==0:
            factors.append(n)
    for y in factors:
            prime(x,y)

prime_factor(int(input("prime:")))
print("Factors:" + factors)
print("Prime factors:" + primes)
input(" ")


